<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml" controllerName="open.sap.AppUI5.controller.ProductDetails">
	<App pages="{path:'/ProductSet',parameters:{expand:'ToSalesOrderLineItems'}}">
		<pages>
			<Page title="Title">
				<content>
				    <sap.ui.layout.form:Form editable="true" xmlns:sap.ui.layout.form="sap.ui.layout.form" id="form0">
				        <sap.ui.layout.form:formContainers>
				            <sap.ui.layout.form:FormContainer title="Title" id="container0">
				                <sap.ui.layout.form:formElements>
				                    <sap.ui.layout.form:FormElement label="Label" id="element0">
				                        <sap.ui.layout.form:fields>
				                            <Input width="100%" id="input0"/>
				                        </sap.ui.layout.form:fields>
				                    </sap.ui.layout.form:FormElement>
				                    <sap.ui.layout.form:FormElement id="element1">
				                        <sap.ui.layout.form:fields>
				                            <DatePicker value="{ChangedAt}" width="100%" id="picker0"/>
				                        </sap.ui.layout.form:fields>
				                    <sap.ui.layout.form:label>
				                        <Label text="{/#Product/ChangedAt/@sap:label}" id="label0"/>
				                    </sap.ui.layout.form:label>
				                    </sap.ui.layout.form:FormElement>
				                    <sap.ui.layout.form:FormElement id="element2">
				                        <sap.ui.layout.form:fields>
				                            <Input value="{ProductID}" width="100%" id="input1"/>
				                        </sap.ui.layout.form:fields>
				                    <sap.ui.layout.form:label>
				                        <Label text="{/#Product/ProductID/@sap:label}" id="label1"/>
				                    </sap.ui.layout.form:label>
				                    </sap.ui.layout.form:FormElement>
				                    <sap.ui.layout.form:FormElement id="element3">
				                        <sap.ui.layout.form:fields>
				                            <Input value="{SupplierID}" width="100%" id="input2"/>
				                        </sap.ui.layout.form:fields>
				                    <sap.ui.layout.form:label>
				                        <Label text="{/#Product/SupplierID/@sap:label}" id="label2"/>
				                    </sap.ui.layout.form:label>
				                    </sap.ui.layout.form:FormElement>
				                    <sap.ui.layout.form:FormElement id="element4">
				                        <sap.ui.layout.form:fields>
				                            <Input value="{SupplierName}" width="100%" id="input3"/>
				                        </sap.ui.layout.form:fields>
				                    <sap.ui.layout.form:label>
				                        <Label text="{/#Product/SupplierName/@sap:label}" id="label3"/>
				                    </sap.ui.layout.form:label>
				                    </sap.ui.layout.form:FormElement>
				                </sap.ui.layout.form:formElements>
				            </sap.ui.layout.form:FormContainer>
				        </sap.ui.layout.form:formContainers>
				        <sap.ui.layout.form:layout>
				            <sap.ui.layout.form:ResponsiveGridLayout id="layout0"/>
				        </sap.ui.layout.form:layout></sap.ui.layout.form:Form>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>